{
    "log": {
        "access": "",
        "error": "",
        "loglevel": "warning",
        "dnsLog": false
    },
    "dns": {
        "servers": [
            "https+local://1.1.1.1/dns-query"
        ]
    },
    "routing": {
        "domainStrategy": "Asls",
        "rules": [
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads-all",
                    "geosite:cn",
                    "geosite:category-games@cn"
                ],
                "outboundTag": "block"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:cn",
                    "geoip:private"
                ],
                "outboundTag": "block"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "/dev/shm/vless.socket",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "chika"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "grpc",
                "tlsSettings": {
                    "rejectUnknownSni": true
                },
                "grpcSettings": {
                    "serviceName": "lovelive"
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            },
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ]
}
